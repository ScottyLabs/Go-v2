generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Route {
  id          String    @id @default(cuid())
  path        String    @unique
  location    String
  description String
  hits        Int       @default(0)
  expiration  DateTime?
  createdAt   DateTime  @default(now())
}
